</$objtype/mkfile
LIB=libutil.a$O

OFILES=

</sys/src/cmd/mklib

SBASESRC=../../../external/sbase

CPPFLAGS =\
	-D_DEFAULT_SOURCE \
	-D_NETBSD_SOURCE \
	-D_BSD_SOURCE \
	-D_XOPEN_SOURCE=700 \
	-D_FILE_OFFSET_BITS=64

CFLAGS=-c $CPPFLAGS -I$SBASESRC -I$SBASESRC/libutil -D_POSIX_SOURCE -D_BSD_EXTENSION

%.$O: $SBASESRC/libutil/%.c
	$CC $CFLAGS $SBASESRC/libutil/$stem.c

nuke:V:
	mk clean
	rm -f $LIB
